.page {
  background-color: rgba(238,238,238,1.000000);
  position: relative;
  width: 750rpx;
  display: flex;
  height:100%;
  flex-direction: column;
}
.card{
  background-color: rgba(255,255,255,1.000000);
  position: relative;
  width: 90%;
  display: flex;
  flex-direction: column;
  margin: 30rpx auto;
  border-radius: 15px;
}
.tabs{
  line-height: 60px!important;
  font-size: 18px!important;

}
.tab{
  height:  60px!important;
  color: red;
}
.titleText{
  font-size: 24px;
  font-weight: 700;
  color: #000;
}
.descText{
  margin-top: 4px;
  color: rgb(107 114 128);
  font-size: 18px;
  line-height: 36px;

}
.prices{
  display: flex;
  justify-content:flex-end;
 line-height: 10px;
 margin-top: -12px;
  font-size: 24px;
  color: red;
  font-weight:900;
}